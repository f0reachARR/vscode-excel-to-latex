{
  "name": "excel-to-latex-copypaste",
  "displayName": "excel-to-latex-copypaste",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:excel-to-latex-copypaste.copyTableToLaTeX"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "excel-to-latex-copypaste.copyTableToLaTeX",
        "title": "Paste table from Excel"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "excel-to-latex-copypaste.copyTableToLaTeX",
          "when": "editorLangId == latex",
          "group": "latex"
        }
      ]
    },
    "keybindings": [
      {
        "command": "excel-to-latex-copypaste.copyTableToLaTeX",
        "key": "ctrl+alt+v",
        "mac": "cmd+alt+v",
        "when": "editorTextFocus && resourceLangId == latex"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "debug-compile": "yarn run compile -- --sourcemap",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "tsc -p . && node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "esbuild": "^0.14.34",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "prettier": "^2.6.2",
    "typescript": "^4.5.5"
  }
}
